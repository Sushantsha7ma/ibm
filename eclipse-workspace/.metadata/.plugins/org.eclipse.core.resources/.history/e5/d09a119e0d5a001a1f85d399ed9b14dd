package Calculator;

public class Calculator 
{
	public int addition(int... a)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}
	public int substraction(int a,int b)
	{
		return a-b;
	}
	public int multiplication(int a,int b)
	{
		return a*b;
	}
	public int division(int a,int b)
	{
		return a/b;
	}

}
