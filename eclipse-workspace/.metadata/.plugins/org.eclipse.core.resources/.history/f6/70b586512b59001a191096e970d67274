package D8Threads;

class Bank implements Runnable
{
	static int count;

	@Override
	public void run() {
		for(int i=0;i<1;i++)
		{
			count++;
		}
		
	}

}
public class MainBank
{
	
	public static void main(String[] args) 
	{
		Thread t1 = new Thread(new Bank());
		Thread t2 = new Thread(new Bank());
		
		t1.start();
		t2.start();
		
	}
}
