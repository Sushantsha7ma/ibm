package D6Collection;

public class TestEmployee 
{
	public static void main(String[] args) 
	{
		Employee e1 = new Employee("Mark",31);
		Employee e2 = new Employee("Mahendra",30);
		Employee e3 = new Employee("Mark",31);
		
		if(e1.equals(e2))
		{
			System.out.println("1st and 2nd same");
		}
		else if(e1.equals(e3))
		{
			System.out.println("1st and 3rd same");
		}
		else if(e2.equals(e3))
		{
			System.out.println("2nd and 3rd same");
		}
		else
		{
			System.out.println("not same");
		}
	}
	
	
}
